<view class="container" style="color:black">
  <view class="doc-title">热门城市</view>
  <view class="zan-panel">
    <view class="zan-cell box">
      <view class="col-25" wx:for="{{Cities}}" wx:key="*this">
        <view class="selector {{item.check?'active':''}}" data-check="{{item.check}}" data-id="{{item.id}}" bindtap="checkChange">{{item.name}}</view>
      </view>
    </view>
  </view>
</view>