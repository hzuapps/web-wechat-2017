<view class='page_header'>
  <view class='page_header_text'>搞笑图片</view>
  <scroll-view class='page_body' scroll-y='true'>
    <navigator  wx:for="{{gaoxiao_img}}" >
      <view class='items'>
          <view class='meta'>
            <view class='title'>{{item.title}}</view>
            
            <image bindtap='imgYu' class='img' src='{{item.img}}' ></image>
            <view class='time'>{{item.ct}}</view>
            <view class='kong'></view>
            
          </view>
      </view>
    </navigator>
  </scroll-view>
</view>